<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <keyboard>

    <key n="100">
      <name>d</name>
      <desc>Windows</desc>
      <binding>
        <command>property-toggle</command>
        <property>/controls/doors/windows</property>
      </binding>
    </key>

    <key n="68">
      <name>D</name>
      <desc>Bay</desc>
      <binding>
        <command>property-toggle</command>
        <property>/controls/doors/bay</property>
      </binding>
    </key>

    <key n="103">
      <name>g</name>
      <desc>gear up</desc>
      <binding>
        <command>nasal</command>
        <script>
          <![CDATA[
controls.gearDown(-1);
          ]]>
        </script>
      <condition>
        <not>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </not>
      </condition>
      </binding>
    </key>



    <key n="115">
      <name>s</name>
      <desc>Fire Starter on Selected Engine(s)</desc>
      <binding>
        <command>property-assign</command>
        <property>/controls/switches/starter</property>
        <value type="int">1</value>
      </binding>
      <binding>
        <command>nasal</command>
        <script>controls.startEngine()</script>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>/controls/switches/starter</property>
          <value type="int">0</value>
        </binding>
        <binding>
          <command>nasal</command>
          <script>props.setAll("/controls/engines/engine", "starter", 0)</script>
        </binding>
      </mod-up>
    </key>
