<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <radar>
    <range type="int">160</range>
  </radar>
  <my_aircraft>

    <target_positions>
      <controls>
        <enabled type="bool">1</enabled>
        <targets_are_mp type="bool">1</targets_are_mp>
      </controls>
      <view>
        <pitch-offset-deg alias="/sim/current-view/pitch-offset-deg" />
        <heading-offset-deg alias="/sim/current-view/heading-offset-deg" />
      </view>
    </target_positions>

    <map>
      <controls>
        <serviceable type="bool">1</serviceable>
        <zoom_primary type="int">12</zoom_primary>
        <zoom_secondary type="int">8</zoom_secondary>
      </controls>
    </map>

    <mfd_left>
      <current_lb1_set type="string">AP</current_lb1_set>
      <current_lb7_set type="string">NAV-COM</current_lb7_set>
      <current_lb8_set type="string"></current_lb8_set>
    </mfd_left>

    <chrono>
      <et-hour type="int">0</et-hour>
      <et-min type="int">0</et-min>
      <et-sec type="int">0</et-sec>
      <et-msec type="int">0</et-msec>
      <is-running type="bool">0</is-running>
      <is-hold type="bool">0</is-hold>
      <is-pause type="bool">0</is-pause>
    </chrono>

  </my_aircraft>

  <attitude-indicator>
    <horizon-offset-deg type="float">0</horizon-offset-deg>
  </attitude-indicator>

</PropertyList>
