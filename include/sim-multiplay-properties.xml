    <float n="8" alias="/gear/gear[2]/wow" />
    <float n="9" alias="/gear/gear[0]/rollspeed-ms" />
    <float n="10" alias="/gear/gear[1]/rollspeed-ms" />
    <float n="11" alias="/gear/gear[2]/rollspeed-ms" />
    <float n="12" alias="/velocities/airspeed-kt" />
    <float n="13" alias="/surface-positions/left-aileron-pos-norm" />
    <float n="14" alias="/gear/gear[1]/compression-norm" /> <!-- right (star) -->
    <float n="15" alias="/gear/gear[2]/compression-norm" /> <!-- left (port) -->
    <float n="16" alias="/gear/gear[0]/caster-angle-deg" />
    <float n="17" alias="/gear/gear[0]/compression-norm" /> <!-- nose -->
    <!-- For Bombable -->
    <int type="int" n="10" alias="controls/armament/trigger">0</int>
    <string n="9" type="string" />
  </generic>

</PropertyList>


