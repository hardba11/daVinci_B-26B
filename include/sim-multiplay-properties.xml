<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chat_display>1</chat_display>
  <generic>
    <int n="0" alias="/controls/switches/nav-lights-switch" />
    <int n="1" alias="/controls/switches/beacon-switch" />
    <int n="2" alias="/controls/switches/landing-lights-switch" />
    <int n="3" alias="/controls/switches/strobe-switch" />
    <int n="4" alias="/controls/smoke" />
    <int type="int" n="5">0</int>
    <int type="bool" n="6">1</int>
    <int n="7" alias="/sim/crashed" />
    <int n="8" alias="/systems/electrical/outputs/nav-lights">0</int>  <!-- 8 nav-lights -->
    <int n="9" alias="/sim/model/lights/beacon/state">false</int>      <!-- 9 strobe-lights -->
    <int n="10" alias="/controls/electric/key-pos">0</int>             <!-- 10 key position -->
    <float n="0" alias="/sim/model/door-positions/windows-position-norm" />
    <float n="1" alias="/sim/model/door-positions/bay-position-norm" />
    <float n="2" alias="/position/gear-agl-m">0</float>   <!-- altitude for MP shadow -->
    <float n="3" alias="/position/altitude-ft">0</float>  <!-- altitude for Smoke/Trail -->
