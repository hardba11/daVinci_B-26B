<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <assistance type="bool">0</assistance>
  <copilot type="bool">1</copilot>

  <gear>
    <brake-parking type="double">1.0</brake-parking>
    <tailwheel-lock>true</tailwheel-lock>
  </gear>

  <engines>
    <engine n="0"> <!-- RIGHT -->
      <magnetos type="int">0</magnetos>
      <!--boost type="double">0.45</boost-->
      <boost type="double">1</boost>
      <blower type="int">0</blower>
      <fuel-pump type="bool">1</fuel-pump>
    </engine>
    <engine n="1"> <!-- LEFT -->
      <magnetos type="int">0</magnetos>
      <!--boost type="double">0.45</boost-->
      <boost type="double">1</boost>
      <blower type="int">0</blower>
      <fuel-pump type="bool">1</fuel-pump>
    </engine>
  </engines>

  <flight>
    <slipstream type="double">1.0</slipstream>
    <aileron-effectiveness type ="double">0.4</aileron-effectiveness>
  </flight>

  <light>
    <instrument-norm type="float">1.0</instrument-norm>
    <panel-norm type="float">1.0</panel-norm>
    <cabin-norm type="float">1.0</cabin-norm>
    <nav type="bool">0</nav>
    <pos type="bool">0</pos>
    <strobe type="bool">0</strobe>
    <landing type="bool">0</landing>
    <taxi type="bool">0</taxi>
  </light>

  <doors>
    <!--canopy-pos-norm type="float">1.0</canopy-pos-norm-->
    <windows type="int">0</windows>
    <bay type="int">0</bay>
    <scale type="int">0</scale>
    <door1 type="int">0</door1>
    <door2 type="int">0</door2>
    <door3 type="int">0</door3>
  </doors>

  <smoke>0</smoke>

  <fuel>
    <indicator>
      <tank-selected>0</tank-selected>
    </indicator>
    <switch-position-port>1</switch-position-port>
    <switch-position-starb>1</switch-position-starb>
  </fuel>

</PropertyList>


