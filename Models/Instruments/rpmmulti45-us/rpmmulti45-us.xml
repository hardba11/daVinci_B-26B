<?xml version="1.0"?>
<PropertyList>

 <path>rpmmulti45-us.ac</path>

 <animation>
  <type>material</type>
  <object-name>case0</object-name>
  <object-name>case1</object-name>
  <object-name>screws</object-name>
   </animation>
 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>needle1</object-name>
  <object-name>needle2</object-name>
  <emission>
       <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle1</object-name>
  <property>/engines/engine/rpm</property>
  <min>0</min>
  <max>4500</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-162</dep>
   </entry>
   <entry>
    <ind>4500</ind>
    <dep>162</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle2</object-name>
  <property>/engines/engine[1]/rpm</property>
  <min>0</min>
  <max>4500</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-162</dep>
   </entry>
   <entry>
    <ind>4500</ind>
    <dep>162</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


</PropertyList>


