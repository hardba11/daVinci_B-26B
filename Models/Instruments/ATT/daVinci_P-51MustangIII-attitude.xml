<?xml version="1.0"?>
<PropertyList>

 <path>MustangIII_VooDoo-attitude.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Face</object-name>
  <property>/instrumentation/attitude-indicator/indicated-roll-deg</property>
  <center>
     <x-m>0.00</x-m>
     <y-m>-0.005</y-m>
     <z-m>-0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>OnOffKnob</object-name>
  <property>/instrumentation/attitude-indicator/serviceable</property>
  <factor>60.0</factor>
  <center>
     <x-m>0.00</x-m>
     <y-m>0.041</y-m>
     <z-m>-0.045</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
