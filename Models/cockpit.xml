<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <!-- nose paroie droite // instruments vitesse et altitude -->
  <model>
    <path>Aircraft/daVinci_B-26B/instruments/asi/asi.xml</path>
    <offsets>
      <x-m>-1.286</x-m>
      <y-m>0.825</y-m>
      <z-m> -0.398</z-m>
      <heading-deg>-75</heading-deg>
      <pitch-deg>9.00</pitch-deg>
    </offsets>
  </model>
  <model>
    <path>Aircraft/daVinci_B-26B/instruments/alt/alt.xml</path>
    <offsets>
      <x-m> -1.1897</x-m>
      <y-m>0.8506</y-m>
      <z-m> -0.397</z-m>
      <heading-deg>-76</heading-deg>
      <pitch-deg>8.00</pitch-deg>
    </offsets>
  </model>

  <!-- cockpit elements -->
  <model>
    <name>staryoke</name>
    <path>Aircraft/daVinci_B-26B/models/cockpit/staryoke/staryoke.xml</path>
    <offsets>
      <x-m>-2.310</x-m>
      <y-m>0.340</y-m>
      <z-m>-0.700</z-m>
    </offsets>
  </model>
  <model>
    <name>portyoke</name>
    <path>Aircraft/daVinci_B-26B/models/cockpit/portyoke/portyoke.xml</path>
    <offsets>
      <x-m>-2.310</x-m>
      <y-m>0.340</y-m>
      <z-m>-0.700</z-m>
    </offsets>
  </model>
  <model>
    <name>pedals</name>
    <path>Aircraft/daVinci_B-26B/models/cockpit/pedals/pedals.xml</path>
    <offsets>
      <x-m>-0.175</x-m>
      <y-m>-0.090</y-m>
      <z-m>-0.670</z-m>
    </offsets>
  </model>
  <model>
    <name>starpedals</name>
    <path>Aircraft/daVinci_B-26B/models/cockpit/starpedals/starpedals.xml</path>
    <offsets>
      <x-m>-0.175</x-m>
      <y-m>0.900</y-m>
      <z-m>-0.670</z-m>
    </offsets>
  </model>
  <model>
    <name>throttleblock</name>
    <path>Aircraft/daVinci_B-26B/models/cockpit/throttleblock/throttleblock.xml</path>
    <offsets>
      <x-m>-2.310</x-m>
      <y-m>0.340</y-m>
      <z-m>-0.700</z-m>
    </offsets>
  </model>

<!-- lights -->
  <animation>
    <type>material</type>
    <object-name>throttleblock</object-name>
    <emission>
      <factor-prop>/sim/model/rendering/relative-cabin-luminosity</factor-prop>
      <red>.04</red>
      <green>.14</green>
      <blue>.14</blue>
    </emission>
  </animation>

</PropertyList>


