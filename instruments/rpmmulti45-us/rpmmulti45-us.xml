<?xml version="1.0"?>
<PropertyList>
  <path>rpmmulti45-us.ac</path>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <emission>
      <factor-prop>/sim/model/rendering/relative-instrument-luminosity</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
    <condition>
      <property>/sim/model/rendering/relative-instrument-luminosity</property>
    </condition>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>needle1</object-name>
    <object-name>needle2</object-name>
    <emission>
      <factor-prop>/sim/model/rendering/relative-cabin-luminosity</factor-prop>
      <red>.04</red>
      <green>.14</green>
      <blue>.14</blue>
    </emission>
    <condition>
      <not><property>/sim/model/rendering/relative-instrument-luminosity</property></not>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>/engines/engine/rpm</property>
    <min>0</min>
    <max>4500</max>
    <interpolation>
      <entry><ind>0</ind><dep>-162</dep></entry>
      <entry><ind>4500</ind><dep>162</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>needle2</object-name>
    <property>/engines/engine[1]/rpm</property>
    <min>0</min>
    <max>4500</max>
    <interpolation>
      <entry><ind>0</ind><dep>-162</dep></entry>
      <entry><ind>4500</ind><dep>162</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>


