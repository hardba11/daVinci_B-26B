<?xml version="1.0"?>
<PropertyList>
  <path>compass.ac</path>
  <animation>
    <type>material</type>
    <object-name>Compass</object-name>
    <emission>
      <factor-prop>/sim/model/rendering/relative-instrument-luminosity</factor-prop>
      <red>   0.017</red>
      <green> 0.60 </green>
      <blue>  0.65 </blue>
    </emission>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Compass</object-name>
    <property>/instrumentation/heading-indicator/indicated-heading-deg</property>
    <center>
      <x-m>-0.02</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>

</PropertyList>


