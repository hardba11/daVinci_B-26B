<?xml version="1.0" encoding="UTF-8"?>

<PropertyList> 
  <fx>

    <!-- ~~~~~~~~~~~~~~~~~~ aerodynamic noises internal -->
    <aero-int>
      <name>aero int</name>
      <path>rumble.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/sim/current-view/internal</property>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0012</factor>
        <min>0</min>
        <max>0.9</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.001</factor>
        <min>0</min>
        <max>2</max>
      </pitch>
    </aero-int>

    <engine-int>
      <name>engine idle rpm int</name>
      <mode>looped</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_idle_rpm.wav</path>
      <condition>
        <property>/sim/current-view/internal</property>
        <property>/engines/engine/running</property>
        <less-than> 
          <property>/engines/engine/rpm</property>
          <value>1500</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine/rpm</property>
          <value>100</value>
        </greater-than>
        <greater-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>2.0</value>
        </greater-than>
      </condition>
      <volume>
        <property>/engines/engine/torque-ftlb</property>
        <factor>0.00035</factor>
        <min>0.2</min>
        <max>0.5</max>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.001</factor>
        <min>1</min>
        <max>3</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-int>

    <engine-ext>
      <name>engine idle rpm</name>
      <mode>looped</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_idle_rpm.wav</path>
      <condition>
        <not><property>/sim/current-view/internal</property></not>
        <property>/engines/engine/running</property>
        <less-than> 
          <property>/engines/engine/rpm</property>
          <value>1500</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine/rpm</property>
          <value>100</value>
        </greater-than>
        <greater-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>2.0</value>
        </greater-than>
      </condition>
      <volume>
        <property>/engines/engine/torque-ftlb</property>
        <factor>0.00035</factor>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.001</factor>
        <min>1</min>
        <max>3</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-ext>

    <engine-4-int>
      <name>engine high rpm int</name>
      <mode>looped</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_high_rpm.wav</path>
      <condition> 
        <property>/sim/current-view/internal</property>
        <property>/engines/engine/running</property>
        <greater-than-equals> 
          <property>/engines/engine/rpm</property>
          <value>1500</value>
        </greater-than-equals>
      </condition>
      <volume>
        <property>/engines/engine/torque-ftlb</property>
        <factor>0.00035</factor>
        <min>0.2</min>
        <max>0.5</max>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.000462</factor>
        <min>0.7</min>
        <max>3.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-4-int>

    <engine-4-ext>
      <name>engine high rpm</name>
      <mode>looped</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_high_rpm.wav</path>
      <condition> 
        <not><property>/sim/current-view/internal</property></not>
        <property>/engines/engine/running</property>
        <greater-than-equals> 
          <property>/engines/engine/rpm</property>
          <value>1500</value>
        </greater-than-equals>
      </condition>
      <volume>
        <property>/engines/engine/torque-ftlb</property>
        <factor>0.00035</factor>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <pitch> 
        <property>/engines/engine/rpm</property>
        <factor>0.000462</factor>
        <min>0.7</min>
        <max>3.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine-4-ext>

    <crank>
      <name>engstart</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_engine_start.wav</path>
      <condition>
        <property>/engines/engine[0]/cranking</property>
        <not>
          <property>/engines/engine[0]/running</property>
        </not>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.3</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>2.5</factor>
        <max>1.5</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>
    <crank>
      <name>cough</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_engine_start.wav</path>
      <property>/engines/engine/running</property>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.2</min>
        <max>0.5</max>
      </volume>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </crank>
    <crank>
      <name>starter</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_engine_start.wav</path>
      <mode>looped</mode>
      <property>/engines/engine/cranking</property>
      <volume>
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.4</min>
        <max>0.8</max>
      </volume>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
    </crank>

    <shutdown>
      <name>shutdown</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_shutdown.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine[0]/magnetos</property>
          <value>0</value>
        </equals>
        <greater-than>
          <property>/engines/engine/rpm</property>
          <value>300</value>
        </greater-than>
        <volume>
          <property>engines/engine/rpm</property>
          <factor>-0.0005</factor>
          <min>0.2</min>
          <max>1</max>
          <offset>1.0</offset>
        </volume>
      </condition>
    </shutdown>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition> 
        <or> 
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
        <property>/sim/current-view/internal</property>
      </condition>
      <volume> 
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.10</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <gear> 
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_gear_down.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
        <greater-than>
          <property>/velocities/airspeed-kt</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.3</factor>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <max>1.0</max>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume> 
        <factor>0.4</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </flap>

    <canopy-open>
      <name>canopy slide open</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_canopy.wav</path>
      <condition>
        <greater-than>
          <property>/sim/model/door-positions/windows-position-norm</property>
          <value>0.1</value>
        </greater-than>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </canopy-open>
    <canopy-close>
      <name>canopy slide close</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_canopy.wav</path>
      <condition>
        <less-than>
          <property>/sim/model/door-positions/windows-position-norm</property>
          <value>0.3</value>
        </less-than>
      </condition>
      <volume>
        <value>1</value>
      </volume>
    </canopy-close>

    <wind-canopy-closed> 
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <condition>
        <equals>
          <property>/instrumentation/doors/crew/position-norm</property>
          <value>0</value>
        </equals>
      </condition>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.001</factor>
        <offset>1.0</offset>
        <min>0.01</min>
        <max>0.7</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.004</factor>
        <min>0.0</min>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind-canopy-closed>

    <wind-canopy-open> 
      <name>wind-loud</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <condition>
        <greater-than>
          <property>sim/multiplay/generic/float[0]</property>
          <value>0.1</value>
        </greater-than>
      </condition>
      <property>/velocities/airspeed-kt</property>
      <volume> 
        <property>/position/altitude-ft</property>
        <factor>-0.004</factor>
        <offset>1.0</offset>
        <min>0</min>
        <max>0.5</max>
      </volume>
      <volume> 
        <property>/velocities/airspeed-kt</property>
        <factor>0.001</factor>
        <min>0</min>
        <max>0.7</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.003</factor>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind-canopy-open>

    <stall>
      <name>gear up warn</name>
      <mode>looped</mode>
      <path>Sounds/gear-hrn.wav</path>
      <condition>
        <and>
          <less-than-equals> 
            <property>controls/engines/engine/throttle</property>
            <value>0.05</value>
          </less-than-equals>
          <equals>
            <property>controls/gear/gear-down</property>
            <value>0</value>
          </equals>
          <equals>
            <property>sim/alarms/gear-warn</property>
            <value>0</value>
          </equals>
        </and>
      </condition>
      <property>/sim/alarms/gear-warn</property>
      <volume>
        <factor>0.7</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </stall>

    <click>
      <name>mag left click down</name>
      <path>Sounds/click.wav</path>
      <condition>
        <property>controls/engines/engine/mag-switch-left</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>mag left click up</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine/mag-switch-left</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>mag right click down</name>
      <path>Sounds/click.wav</path>
      <condition>
        <property>controls/engines/engine/mag-switch-right</property>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click> 
      <name>mag right click up</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine/mag-switch-right</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>nav lights click down</name>
      <path>Sounds/click.wav</path>
      <condition>
        <property>controls/light/nav</property>
      </condition>
      <volume> 
        <factor>1.0</factor>
        <min>0.1</min>
        <max>0.9</max>
      </volume>
    </click>

    <click>
      <name>nav lights click up</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/light/nav</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
        <min>0.1</min>
        <max>0.9</max>
      </volume>
    </click>
    
    <low-fuel>
      <name>low-fuel</name>
      <mode>looped</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_shutdown.wav</path>
      <condition>
        <less-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>2.0</value>
        </less-than>
        <greater-than>
          <property>/consumables/fuel/total-fuel-gals</property>
          <value>0.1</value>
        </greater-than>
      </condition>
    </low-fuel>

    <no-fuel>
      <name>no-fuel</name>
      <path>Aircraft/daVinci_B-26B/sounds/b26_shutdown.wav</path>
      <condition>
        <less-than>
         <property>/consumables/fuel/total-fuel-gals</property>
        <value>0.1</value>
        </less-than>
      </condition>
    </no-fuel>

    <!-- flyby -->
    <flyby>
      <name>fly-by</name>
      <mode>once</mode>
      <path>Aircraft/daVinci_B-26B/sounds/b26_high_rpm.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>velocities/airspeed-kt</property>
            <value>50</value>
          </greater-than>
          <equals>
            <property>sim/current-view/view-number</property>
            <value>6</value>
          </equals>
        </and>
        <not>
          <property>/sim/current-view/internal</property>
          <property>/engines/engine/out-of-fuel</property>
        </not>
      </condition>
      <volume>
      <property>velocities/airspeed-kt</property>
      <factor>0.001</factor>
      </volume>
    </flyby>



  </fx>


</PropertyList>



